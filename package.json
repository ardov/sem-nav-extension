{
  "name": "sem-nav-extension",
  "version": "0.0.7",
  "description": "An extension to improve Semrush navigation",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ardov/sem-nav-extension"
  },
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "build:watch": "cross-env __DEV__=true vite build -w",
    "build:hmr": "rollup --config utils/reload/rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "wss": "node utils/reload/initReloadServer.js",
    "dev": "npm run build:hmr && (run-p wss build:watch)"
  },
  "type": "module",
  "dependencies": {
    "@nanostores/react": "^0.7.1",
    "cmdk": "^0.2.0",
    "nanostores": "^0.9.3",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/chrome": "^0.0.243",
    "@types/node": "^20.5.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.15",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fs-extra": "^11.1.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.27",
    "prettier": "^3.0.2",
    "rollup": "^3.28.0",
    "sass": "^1.65.1",
    "typescript": "^5.1.6",
    "vite": "3.1.3",
    "ws": "8.13.0"
  }
}
